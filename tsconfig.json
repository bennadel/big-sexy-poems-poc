{
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"DOM",
			"ES6"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"pretty": true,
		"removeComments": false,
		"sourceMap": true,
		"strictNullChecks": false,
		"strictPropertyInitialization": false,
		"suppressImplicitAnyIndexErrors": true,
		"target": "es5",
		"types": [
			"node"
		]
	}
}
